  public static int[] colTotals(int[][] numbers)
  {
    int[] totals = new int[numbers[0].length];
    for (int c = 0; c = numbers[0].length; c++;)
    
  }

  public static void turnUpsideDown(int[][] numbers)
  {
    for (int r = 0; r < numbers.length/2; r++)
    {
      temp = numbers[r];
      numbers[r] = numbers[numbers.length - 1 - r];
      numbers[numbers.length-1-r] = temp;
    }
  }
  
  public static void flipSideways(int[][] numbers)
  {
    for (int r = 0; r < numbers.length/2; r++)
    {
      temp = numbers[r];
      numbers[r] = numbers[numbers.length - 1 - r];
      numbers[numbers.length-1-r] = temp;
    }
  }
}

